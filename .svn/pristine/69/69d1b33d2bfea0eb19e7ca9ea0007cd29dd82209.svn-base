	function uf_imprimir()
	{
			f=document.formulario;
			li_imprimir=f.imprimir.value;
			if(li_imprimir==1)
			{
				if(f.chkexcluir[0].checked)
				{
					excluir=1;
				}
				else
				{
					excluir=0;
				}
				codprobendes=f.txtcoddes.value;
				codprobenhas=f.txtcodhas.value;
				fechades=f.txtfechades.value;
				fechahas=f.txtfechahas.value;
				pantalla="reportes/sigesp_cxp_rpp_cxpresumido.php?excluir="+excluir+"&fechades="+fechades+"&fechahas="+fechahas+"";
