function ue_nuevo()
{
  f=document.form1;
  li_incluir=f.incluir.value;
  if (li_incluir==1)
	 {	
       f.operacion.value="NUEVO";
       f.txtcodigo.value="";
	   f.txtdenominacion.value="";
	   f.txtdenominacion.focus(true);
	   f.action="sigesp_sep_d_tipo.php";
	   f.submit();
     }
  else
     {
       alert("No tiene permiso para realizar esta operación");
	 }
}

